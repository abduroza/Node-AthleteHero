{
  "name": "athletehero",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "develop": "nodemon ./bin/www",
    "test": "mocha --timeout 10000 --recursive ./test --exit",
    "coverage": "nyc --reporter=html --reporter=text mocha --timeout 10000 --recursive ./test --exit"
  },
  "nyc": {
    "_comment": "Purpose of this part: to do code coverage within Mocha/Chai using Istanbul",
    "register": "babel-register",
    "include": [
      "**/*.js",
      "./*.js"
    ],
    "exclude": [
      "**/node_modules/**",
      "**/test/**",
      "**/coverage/**"
    ],
    "all": true
  },
  "dependencies": {
    "@sendgrid/mail": "^6.4.0",
    "bcryptjs": "^2.4.3",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "cloudinary": "^1.15.0",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "datauri": "^2.0.0",
    "debug": "~2.6.9",
    "dotenv": "^8.1.0",
    "express": "~4.16.1",
    "faker": "^4.1.0",
    "http-errors": "~1.6.3",
    "imagekit": "^1.5.0",
    "istanbul": "^0.4.5",
    "jade": "~1.11.0",
    "jsonwebtoken": "^8.5.1",
    "mocha": "^6.2.0",
    "mongoose": "^5.7.5",
    "mongoose-unique-validator": "^2.0.3",
    "mongoose-validator": "^2.1.0",
    "morgan": "~1.9.1",
    "multer": "^1.4.2",
    "nodemon": "^1.19.1",
    "nyc": "^14.1.1"
  }
}
